{
  "name": "{{module.getComposerExtensionName() }}",
  "description": "{{module.getExtensionName() }} magento 2 extension",
  "require": {
{% for dep in module.getComposerDependencies() %}
    "{{ dep }}": "*",
{% endfor %}
    "php": "~7.1.3||~7.2.0||~7.3.0",
  },
  "type": "magento2-module",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "{{ module.getExtensionName('\\\\') }}\\": ""
    }
  }
}
