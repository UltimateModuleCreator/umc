{% if module.hasFrontend() %}
<?xml version="1.0"?>{{ module.getFormattedLicense('xml')|raw }}
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <router id="standard">
        <route id="{{ module.getNamespace()|lower }}_{{ module.getModuleName()|lower }}" frontName="{{ module.getFrontKey()|lower }}">
            <module name="{{ module.getNamespace() }}_{{ module.getModuleName() }}" />
        </route>
    </router>
</config>
{% endif %}
