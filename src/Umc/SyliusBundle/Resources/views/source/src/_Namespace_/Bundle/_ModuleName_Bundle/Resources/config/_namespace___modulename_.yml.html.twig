sylius_resource:
    resources:
{% for entity in module.getEntities() %}
        {{ module.getModuleName()|snake }}.{{ entity.getNameSingular()|snake }}:
            classes:
                model: App\{{ module.getNamespace() }}\Bundle\{{ module.getModuleName() }}Bundle\Entity\{{ entity.getNameSingular()|camel|ucfirst }}
{% endfor %}
