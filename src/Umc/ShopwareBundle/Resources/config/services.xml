<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="false" autowire="true" autoconfigure="true" />
        <service id="shopware.platform.pool.provider" class="App\Umc\CoreBundle\Config\Provider">
            <argument key="$file" type="string">%kernel.project_dir%/src/Umc/ShopwareBundle/Resources/config/platform.yml</argument>
            <tag name="platform.pool.provider" />
        </service>
    </services>
</container>
